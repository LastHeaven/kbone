    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "@babel/preset-env": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
